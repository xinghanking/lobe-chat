{
  "name": "LobeChat Dev",
  "image": "mcr.microsoft.com/devcontainers/typescript-node:22",
  "postCreateCommand": "curl -fsSL https://bun.sh/install | bash && npm install -g pnpm && pnpm install",
  "customizations": {
    "vscode": {
      "extensions": ["dbaeumer.vscode-eslint", "esbenp.prettier-vscode"]
    }
  },
  "portsAttributes": {
    "3010": { "label": "Application", "onAutoForward": "openPreview" }
  }
}
